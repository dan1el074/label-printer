{
    "name": "imprime-etiquetas",
    "version": "0.0.1",
    "description": "Software desktop que tem como objetivo imprimir etiquetas de diversos tipos e tamanhos",
    "main": "main.js",
    "scripts": {
        "build:prod": "electron-packager .\\ ImprimeEtiqueta --platform=win32 --arch=x64 --out=dist --overwrite --icon=public/images/icon.ico",
        "config:prod": "del /Q dist\\ImprimeEtiqueta-win32-x64\\LICENSE dist\\ImprimeEtiqueta-win32-x64\\LICENSES.chromium.html dist\\ImprimeEtiqueta-win32-x64\\version",
        "clean-dist": "cd dist/ImprimeEtiqueta-win32-x64/locales && for %f in (*) do if /I not \"%f\"==\"en-US.pak\" if /I not \"%f\"==\"pt-BR.pak\" del /Q \"%f\""
    },
    "keywords": [
        "Electron",
        "typescript"
    ],
    "author": "Daniel Rodrigues de Vargas",
    "devDependencies": {
        "@electron-forge/cli": "^7.3.0",
        "@types/node": "^20.14.10",
        "electron": "^28.1.0",
        "globals": "^15.8.0",
        "typescript": "^4.8.3"
    },
    "dependencies": {
        "fs": "^0.0.1-security",
        "pdf-lib": "^1.17.1",
        "pdf-to-printer": "^5.6.0"
    }
}
